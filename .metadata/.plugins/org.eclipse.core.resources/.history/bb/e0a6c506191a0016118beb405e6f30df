#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <string.h>

typedef struct soldado LSoldado;
struct soldado {
	int numero;
	LSoldado* proximo;
};

int main(void) {
	printf("O Problema de Josephus\n\nInforme um número para sortear um primeiro soldado.");
	int numbersOfSoldiers = 0;
	scanf("%d", &numbersOfSoldiers);

	printf("\n\n%d", numbersOfSoldiers);

	return 0;
}
