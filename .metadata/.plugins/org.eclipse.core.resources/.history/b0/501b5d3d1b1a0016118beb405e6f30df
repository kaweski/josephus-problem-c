typedef struct soldado SLista;
struct soldado {
	int numero;
	SLista* proximo;
};

insereSoldadoNoCirc(int num, SLista* lista);

int main(void) {

	int numSoldados = 0;
	printf("O Problema de Josephus\n\nNúmero de soldados utilizados:");
	scanf("%d", &numSoldados);

	char soldados[numSoldados];
	SLista* circuloSoldados;

	insereSoldadoNoCirc(numSoldados, circuloSoldados);

	return 0;
}

