#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <string.h>

typedef struct soldier LSoldier;
struct soldier {
	char data;
	LSoldier* nextSoldier;
};

int main(void) {
	printf("O Problema de Josephus" +
			"Informe um número para sortear um primeiro soldado.");
	int numbersOfSoldiers;
	scanf("%d", &numbersOfSoldiers);
}
